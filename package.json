{
  "name": "vg-coder-cli",
  "version": "2.0.37",
  "description": "ðŸš€ CLI tool to analyze projects, concatenate source files, count tokens, and export HTML with syntax highlighting and copy functionality",
  "main": "src/index.js",
  "bin": {
    "vg": "./bin/vg.js",
    "vg-coder": "./bin/vg-coder.js"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "install:local": "npm install -g .",
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "nodemon src/index.js",
    "build:extension": "cd vetgo-auto && npm run build",
    "build:copy": "node scripts/build.js",
    "build": "npm run build:extension && npm run build:copy && npm run build:inject && npm run install:local",
    "push": "npm run build && npm pack && npm publish",
    "build:inject": "gulp"
  },
  "keywords": [
    "cli",
    "code-analysis",
    "token-counter",
    "ai-helper",
    "terminal",
    "web-terminal"
  ],
  "author": "VG Coder",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "diff2html": "^3.4.52",
    "directory-tree": "^3.5.1",
    "express": "^4.18.2",
    "fs-extra": "^11.3.3",
    "highlight.js": "^11.9.0",
    "ignore": "^5.3.0",
    "markdown-it": "^14.0.0",
    "mermaid": "^11.4.1",
    "node-pty": "^1.1.0",
    "ora": "^5.4.1",
    "path": "^0.12.7",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.3",
