    "gulp-concat": "^2.6.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-replace": "^1.1.4",
    "gulp-terser": "^2.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "webpack": "^5.104.1",
    "webpack-stream": "^7.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
