{
  "name": "vg-coder",
  "version": "1.0.0",
  "author": "VetGo",
  "description": "Vg Coder",
  "scripts": {
    "clean": "rimraf chrome/dist",
    "build": "npm run clean && webpack --config chrome/webpack.config.js",
    "build:prod": "npm run clean && webpack --config chrome/webpack.config.prod.js && npm run pack",
    "pack": "cd chrome/dist && bestzip ../../vg-coder.zip *",
    "deploy-scripts": "node deploy-scripts.js"
  },
  "dependencies": {
    "firebase": "^9.17.2",
    "js-sha256": "^0.9.0",
    "lodash": "^4.17.21",
    "rxjs": "~7.8.0",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.209",
    "bestzip": "^2.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "rimraf": "^4.1.1",
    "ts-loader": "^9.4.2",
    "typescript": "~4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
